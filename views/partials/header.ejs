  <!-- Modal content start -->
    <div class="modal-content">
      <div class="modal-header">
        <!-- Title dynamically filled -->
        <h5 class="modal-title object-info"></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <!-- Modal body start-->
      <div class="modal-body">

        <nav class="nav nav-tabs">
          <a class="nav-item nav-link active" id="firstInfoModalTab" data-toggle="tab" href="#sandfang">Sandfang</a>
          <a class="nav-item nav-link" data-toggle="tab" href="#sandfangTomming">Reg Tømming</a>
          <a class="nav-item nav-link" data-toggle="tab" href="#sandfangLog"><i class="far fa-clock"></i> Log</a>
          <a class="nav-item nav-link" data-toggle="tab" href="#sandfangSkade">Skade</a>
          <a class="nav-item nav-link" data-toggle="tab" href="#sandfangBilde">Bilde</a>
        </nav>
        <!-- Tab content start -->
        <div class="tab-content py-3">
          <!-- Sandfang pane start -->
          <div class="tab-pane active" id="sandfang">
            <div id="sandfangInfo">
              <ul id="sandfangInfoList">
                <li class="sandfangLi">
                  <span class="text-left">Id: </span>
                  <span class="text-right featureId"></span>
                </li>
                <li class="sandfangLi">
                  <span class="text-left">Siste Tømmedato: </span>
                  <span class="text-right last-tomming">Dato Her</span>
                </li>
                <li class="sandfangLi">
                  <span class="text-left">Kritisk merknad</span>
                  <span class="text-right kritiskBool"></span>
                </li>
                </li>
                  <div>
                    <!-- <label for="exampleFormControlTextarea2">Example textarea</label> -->
                    <textarea disabled placeholder="Ingen merknad registrert" class="form-control" id="infoMerknedTextArea" rows="2"></textarea>
                  </div>
                </li>
              </ul>
              <div class="center-btn">
                <button type="button" id="btnEditSandfangInfo" class="btn btn-primary">Editere merknad</button>
              </div>
            </div>

            <!-- Sandfang edit start -->
            <div id="editSandfangInfo">
              <form id=infoForm>
                <div class="form-group">
                  <label for="editMerknedTextArea">Example text area</label>
                  <textarea class="form-control" id="editMerknedTextArea" name="merkned" rows="3" placeholder="Skriv en merknad"></textarea>
                </div>  
                <div class="form-group-check">
                  <input type="checkbox" id="checkKritisk" name="kritisk_merkned">
                  <label class="form-check-label" for="checkKritisk">Kritisk Merkned</label>
                </div>
                <div id="btn-group-sandfang">
                  <button type="submit" class="btn btn-success">Lagre</button>
                  <button type="button" id="hideEditSandfangInfoForm" class="btn btn-danger">Avbryt</button>
                </div>
              </form>
            </div>
            <!-- Sandfang edit end -->


          </div>
          <!-- Sandfang pane end-->
          <!-- Sandfang tomming pane start-->
          <div class="tab-pane" id="sandfangTomming">
            <div id="editTomming">
              <form id="registrerTommingForm">
                <select name="fyllingsgrad" class="form-control form-control-lg">
                  <option selected disabled>Velg fyllnadsnivå ved Tømming</option>
                  <option value="0">Tom</option>
                  <option value="1">1/3</option>
                  <option value="2">2/3</option>
                  <option value="3">Full</option>
                </select>
                <div class="center-btn">
                  <button type="submit" id="registrerTommingForm" class="btn btn-primary">Registrere Tømming</button>
                </div>
              </form>
            </div>
          </div>
          <!-- Sandfang tomming pane end -->
          <!-- Sandfang log pane start -->
          <div class="tab-pane" id=sandfangLog>
            <table class="table table-striped table-sm" id="sandfangLogTable">
              <thead>
                <tr>
                  <th scope="col">#</th>
                  <th scope="col">Tømmedato</th>
                  <th scope="col">Fyllingsgrad</th>
                </tr>
              </thead>
              <tbody>
                <!-- Tømme log data go inserted here from server -->
              </tbody>
            </table>
          </div>
          <!-- Sandfang log pane end -->
          <!-- Sandfang skade pane start -->
          <div class="tab-pane" id="sandfangSkade">
            <div class="container">

              <div id="sandfangSkadeInfo">
                <ul class="list-group skadeLog" id="registrert-skader-list">
